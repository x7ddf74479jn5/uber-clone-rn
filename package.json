{
  "name": "uber-clone-rn",
  "version": "1.0.0",
  "main": "app/App.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint .",
    "prettier": "prettier --check .",
    "fix": "run-s -l -c fix:lint fix:prettier",
    "fix:lint": "eslint --fix .",
    "fix:prettier": "prettier --write ."
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.7.1",
    "expo": "~44.0.0",
    "expo-status-bar": "~1.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-web": "0.17.1",
    "react-redux": "7.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "autoprefixer": "10.4.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tailwindcss": "3.0.0-beta.1",
    "husky": "7.0.4",
    "lint-staged": "12.1.4",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.7",
    "typescript": "~4.3.5"
  },
  "private": true,
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix .",
      "prettier --write ."
    ],
    "**/*.{json,md,mdx,css,html,yml,yaml,scss}": [
      "prettier --write ."
    ]
  }
}
